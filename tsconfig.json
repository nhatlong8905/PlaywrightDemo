{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
           "@uiActions/*":["playwright/actions/*"],
           "@utils/*":["utils/*"],
           "@asserts/*":["playwright/asserts/*"],
        },
        "target": "ESNext",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "sourceMap": true,
        "outDir": "../tests-out",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
    }
}